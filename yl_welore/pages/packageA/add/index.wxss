.nav-wrap {
    position: fixed;
    width: 100%;
    top: 0;
    background: #fff;
    color: #000;
    z-index: 9999999;
}

.i_tag_style {
    font-size: 12px!important;
}

.add_submit {
    background: #ffffff;
    color: #666666;
    border-radius: 2rem;
    border: 1px #666666 solid;
    box-shadow: -3px 5px 10rpx 5rpx#666666;
}

.nav-title {
    position: absolute;
    text-align: center;
    max-width: 377rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    font-size: 36rpx;
    color: #2c2b2b;
    font-weight: 600;
}

.nav-capsule {
    display: flex;
    align-items: center;
    margin-left: 20rpx;
    width: 50rpx;
    justify-content: space-around;
    border-radius: 50%;
    margin-top: 54rpx;
    z-index: 999999999;
}

.navbar-v-line {
    width: 1px;
    height: 32rpx;
    background-color: #F3F3F3;
}

.back-pre {
    width: 32rpx;
    height: 32rpx;
    margin-top: 11rpx;
    margin-left: 0rpx;
}

.show-btn {
    margin-top: 100rpx;
    color: #22cc22;
}

.modal-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
    z-index: 9000;
    color: #fff;
}

.modal-dialog {
    width: 540rpx;
    overflow: hidden;
    position: fixed;
    top: 32%;
    left: 0;
    z-index: 9999;
    background: #f9f9f9;
    margin: -180rpx 105rpx;
    border-radius: 8px;
}

.modal-title {
    padding-top: 50rpx;
    font-size: 36rpx;
    color: #030303;
    text-align: center;
}

.modal-content {
    padding: 20rpx 32rpx;
}

.modal-input {
    display: flex;
    background: #fff;
    border: 2rpx solid #ddd;
    border-radius: 4rpx;
    font-size: 28rpx;
}

.input {
    width: 100%;
    height: 82rpx;
    font-size: 28rpx;
    line-height: 28rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
    color: #333;
}

input-holder {
    color: #666;
    font-size: 28rpx;
}

.modal-footer {
    display: flex;
    flex-direction: row;
    height: 86rpx;
    border-top: 1px solid #dedede;
    font-size: 34rpx;
    line-height: 86rpx;
}

.btn-cancel {
    width: 100%;
    color: #666;
    text-align: center;
    border-right: 1px solid #dedede;
}

.demo-container {
    width: 50vw;
    height: 100vh;
    background: #fff;
}

.container {
    width: 100%;
    background-color: #fff;
    color: #939393;
}

.nav_right {
    flex: 1;
    height: 40em;
    padding: 10px;
    box-sizing: border-box;
    background: #fff;
    overflow-y: scroll;
}

.nav_right .nav_right_items {
    float: left;
    width: 33.33%;
    height: 80px;
    text-align: center;
}

.nav_right .nav_right_items image {
    width: 50px;
    height: 30px;
}

.nav_right .nav_right_items text {
    display: block;
    margin-top: 5px;
    font-size: 10px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}